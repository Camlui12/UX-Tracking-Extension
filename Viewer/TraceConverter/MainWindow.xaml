<Window x:Class="TraceConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TraceConverter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="695.333">
    <Grid>
        <TextBox x:Name="Txt_input" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" TextWrapping="Wrap" Text="Eye Converter" VerticalAlignment="Top" Width="586"/>
        <Button x:Name="Cmd_convert" Content="CONVERT!" HorizontalAlignment="Left" Margin="601,10,0,0" VerticalAlignment="Top" Width="75" Click="Cmd_convert_Click"/>
        <TextBox x:Name="Txt_log" HorizontalAlignment="Left" Height="374" Margin="10,35,0,0" TextWrapping="Wrap" Text="WAVES Systems" VerticalAlignment="Top" Width="666"/>

    </Grid>
</Window>
