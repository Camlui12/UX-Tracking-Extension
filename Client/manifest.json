{
  "name": "AIMT-UXT Client (Eye-Tracking edition)",
  "version": "2.0",
  "description": "Software de captura de dados de interacao.",
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>", "http://*/*", "https://*/*" ],
      "js": ["searchgazer.js", "main.js", "Content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Obrigado por participar!"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "unlimitedStorage",
    "tabs",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
