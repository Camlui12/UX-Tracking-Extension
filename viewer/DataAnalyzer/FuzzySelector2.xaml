<Window x:Class="Lades.WebTracer.FuzzySelector2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lades.WebTracer"
        mc:Ignorable="d"
        Title="FuzzySelector2" SizeToContent="Height">
    <Grid>
        <StackPanel Margin="30" Orientation="Vertical">
            <TextBox x:Name="Txt_tracepaths" Margin="0" TextWrapping="Wrap" MinHeight="200" AcceptsReturn="True"/>
            <Button x:Name="Cmd_start" Content="START" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Bottom" MinWidth="70" Click="Cmd_start_Click"/>

        </StackPanel>

    </Grid>
</Window>
